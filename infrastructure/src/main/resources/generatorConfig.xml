<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="dsql" targetRuntime="MyBatis3DynamicSql">
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/ccb_v2"
                        userId="root"
                        password="gongmeng"/>

        <javaModelGenerator targetPackage="com.se.back.data.repo.db.dataclass" targetProject="src/main/java"/>

        <javaClientGenerator targetPackage="com.se.back.data.repo.db.mapper" targetProject="src/main/java"/>

        <table tableName="alembic_version" schema="ccb_v2">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>

        <table tableName="account" schema="ccb_v2">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>
